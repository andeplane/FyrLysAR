# App Store Connect API Key (Required for releases)
# Get these from https://appstoreconnect.apple.com/ → Users and Access → Keys
APP_STORE_CONNECT_API_KEY_ID=your_key_id_here
APP_STORE_CONNECT_ISSUER_ID=your_issuer_id_here
APP_STORE_CONNECT_API_KEY_CONTENT="-----BEGIN PRIVATE KEY-----
your_p8_file_content_here
-----END PRIVATE KEY-----"

# OpenAI API Key (Optional - for AI-generated release notes)
# Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here
